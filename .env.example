# Oracle Environment Configuration
# Copy this file to .env and fill in your values

# Required: Anthropic API key for Claude AI
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Database Configuration (defaults work with devcontainer)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=oracle
DB_USER=oracle
DB_PASSWORD=oracle_dev_password

# Temporal Configuration
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default

# Server Configuration
PORT=3001

# API Authentication (disabled when empty â€” set for production)
# Comma-separated list of valid API keys
# ORACLE_API_KEYS=key1,key2,key3

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60

# Development Settings
NODE_ENV=development
